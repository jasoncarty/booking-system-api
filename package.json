{
    "name": "booking-system-api",
    "version": "0.0.1",
    "description": "",
    "author": "Jason Carty",
    "license": "MIT",
    "scripts": {
        "build": "gulp && tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "gulp && concurrently --handle-input \"wait-on dist/src/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
        "start:debug": "nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf dist && npm run build",
        "start:prod": "NODE_ENV='production' node dist/src/main.js",
        "lint": "eslint",
        "lint:fix": "eslint --fix .",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "run-s e2e:prepare",
        "e2e:test:execute": "run-s e2e:clean e2e:prepare e2e:test",
        "e2e:test:ci": "run-s e2e:clean e2e:prepare new-ci-tab",
        "e2e:test": "jest --config ./e2e/jest-e2e.json",
        "e2e:prepare": "ts-node ./scripts/prepareTestDatabase.ts",
        "e2e:clean": "ts-node ./scripts/dropTestDB.ts"
    },
    "dependencies": {
        "@hapi/joi": "^15.1.0",
        "@nest-modules/mailer": "^1.1.3",
        "@nestjs/common": "^6.5.0",
        "@nestjs/core": "^6.5.0",
        "@nestjs/platform-express": "^6.5.0",
        "@nestjs/typeorm": "^6.1.2",
        "bcrypt": "^3.0.6",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.9.1",
        "dotenv": "^8.0.0",
        "idx": "^2.5.6",
        "jsonwebtoken": "^8.5.1",
        "pug": "^2.0.4",
        "rand-token": "^0.4.0",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "rxjs": "^6.5.2",
        "typeorm": "^0.2.18",
        "url": "^0.11.0"
    },
    "devDependencies": {
        "@nestjs/testing": "^6.5.0",
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^3.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/chalk": "^2.2.0",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.0",
        "@types/gulp": "^4.0.6",
        "@types/hapi__joi": "^15.0.2",
        "@types/jest": "^24.0.15",
        "@types/joi": "^14.3.3",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/node": "^12.6.2",
        "@types/pg": "^7.4.14",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^1.11.0",
        "@typescript-eslint/parser": "^1.11.0",
        "axios": "^0.19.0",
        "chalk": "^2.4.2",
        "concurrently": "^4.1.1",
        "coveralls": "^3.0.5",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-jest": "^22.7.2",
        "eslint-plugin-prettier": "^3.1.0",
        "gulp": "^4.0.2",
        "jest": "^24.8.0",
        "jest-plugin-unhandled-promise": "^2.9.0",
        "nodemon": "^1.19.1",
        "npm-run-all": "^4.1.5",
        "pg": "^7.11.0",
        "prettier": "1.18.2",
        "supertest": "^4.0.2",
        "ts-jest": "24.0.2",
        "ts-node": "8.3.0",
        "tsconfig-paths": "3.8.0",
        "typescript": "3.5.3",
        "wait-on": "^3.2.0"
    },
    "jest": {
        "setupFiles": [
            "jest-plugin-unhandled-promise/setup"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/e2e/",
            "<rootDir>/dist/"
        ],
        "rootDir": ".",
        "testRegex": ".spec.ts$",
        "coverageDirectory": "<rootDir>/coverage",
        "testEnvironment": "node",
        "preset": "ts-jest"
    }
}